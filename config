
#############  ~/.ssh/config #############33

Host bastionNSO
        Hostname 91.106.198.235
        User ubuntu
        IdentityFile ~/.ssh/id_rsa
        UserKnownHostsFile=~/dev/null
        StrictHostKeyChecking no
        PasswordAuthentication no

Host HAproxy
         Hostname 91.106.198.245
         User ubuntu
         IdentityFile ~/.ssh/id_rsa
         UserKnownHostsFile=~/dev/null
         StrictHostKeyChecking no
         PasswordAuthentication no

Host devA
          Hostname 10.0.1.10
          User ubuntu
          ProxyJump bastionNSO
          IdentityFile ~/.ssh/id_rsa
          UserKnownHostsFile=~/dev/null
          StrictHostKeyChecking no
          PasswordAuthentication no

Host devB
          Hostname 10.0.1.7
          User ubuntu
          ProxyJump bastionNSO
          IdentityFile ~/.ssh/id_rsa
          UserKnownHostsFile=~/dev/null
          StrictHostKeyChecking no
          PasswordAuthentication no


Host devC
          Hostname 10.0.1.8
          User ubuntu
          ProxyJump bastionNSO
          UserKnownHostsFile=~/dev/null
          StrictHostKeyChecking no
          PasswordAuthentication no

